        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cybz_back.mapper.RolePermissionMapper">

    <resultMap id="BaseResultMap" type="com.example.cybz_back.entity.mysql.auth.RolePermission">
        <id property="roleId" column="role_id" jdbcType="BIGINT"/>
        <id property="permId" column="perm_id" jdbcType="BIGINT"/>
